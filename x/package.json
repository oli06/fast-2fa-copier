{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "fast-2fa-copier",
  "title": "Fast 2FA Copier",
  "description": "Quickly copy the latest 2FA from your messages or paste it directly in your browser.",
  "icon": "command-icon.png",
  "author": "oliver_zernikow",
  "owner": "zentri.de",
  "categories": [
    "Applications",
    "Security",
    "Productivity",
    "System"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "copy",
      "title": "Copy 2FA Code",
      "description": "Template for running a script without any view",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.58.0",
    "shell-env": "^4.0.1",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "files": [
    "./assets/"
  ],
  "scripts": {
    "build": "ray build -e ./dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
